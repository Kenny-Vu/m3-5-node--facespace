<%- include("../partials/header")%>
<div class="profile-page--banner"></div>
<div class="profile-page__user">
    <img src="<%= user.avatarUrl %>" alt="" class="profile-page__user--avatar">
    <h2><%= user.name %></h2>
  </div>
  <div class="profile-page__friends">
    <h3><%= user.name%>'s Friends</h3>
    <ul class="profile-page__friends--list">
        <%friendList.forEach(friend=>{ %>
            <li class="friend">
                <img src="<%-friend.avatarUrl%>" alt="">
                <p><%-friend.name%></p>
            </li>
        <%})%>
    </ul>
    <div class="add-friend" id="add-friend">
        <a href=<%-`/addfriend/${id}`%>>
            <%- currentIdVisit %>
        </a></div>
</div>
<%- include("../partials/footer")%>